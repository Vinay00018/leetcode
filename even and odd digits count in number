class Main {
    public static void main(String[] args) {
       int n=2123842;
       int count1=0;
       int count2=0;
       while(n!=0)
       {
           int num=n%10;
           if(num%2==0)
           {
               count1++;
           }
           else
           {
               count2++;
           }
           n=n/10;
       }
       System.out.println(count1+" "+count2);
    }
}
